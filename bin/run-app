# export MONGODB_USERNAME=${MONGODB_USERNAME:="disk_tracking"}
export MONGODB_USERNAME=${MONGODB_USERNAME:="root"}
export RUST_LOG=${RUST_LOG:="debug"}

cargo run --bin wipac-disk-tracking
